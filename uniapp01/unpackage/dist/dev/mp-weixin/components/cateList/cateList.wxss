.cateList .swiper {
  height: 350rpx;
}
.cateList .swiper .swiperImg {
  width: 100%;
  height: 350rpx;
}
.cateList .title {
  font-size: 40rpx;
  line-height: 80rpx;
  color: #333;
  text-align: center;
}
.cateList .desc {
  font-size: 30rpx;
  line-height: 40rpx;
  color: #999;
  text-align: center;
}
.cateList .goodsList {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.cateList .goodsList::after {
  content: '';
  width: 344rpx;
}
.cateList .goodsList .goodsItem {
  width: 344rpx;
  height: 480rpx;
}
.cateList .goodsList .goodsItem .goodsImg {
  width: 344rpx;
  height: 344rpx;
}
.cateList .goodsList .goodsItem .goodsPrice {
  color: #f00;
  font-size: 45rpx;
}

